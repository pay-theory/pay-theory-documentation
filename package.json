{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint -c .eslintrc.json --ext .ts,.tsx './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "check-ts": "tsc --noEmit -p ./tsconfig.json",
    "prettier": "npx prettier -c './src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "npx prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.12.1",
    "@algolia/autocomplete-theme-classic": "^1.12.1",
    "@docusaurus/core": "3.0.0",
    "@docusaurus/plugin-sitemap": "^3.0.0",
    "@docusaurus/preset-classic": "3.0.0",
    "@docusaurus/theme-search-algolia": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "algoliasearch": "^4.20.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "clsx": "^2.0.0",
    "docusaurus2-dotenv": "^1.4.0",
    "dotenv": "^16.3.1",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "docusaurus-plugin-typedoc": "^0.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.3",
    "typedoc-plugin-markdown": "^3.17.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run prettier:fix",
      "npm run lint:fix"
    ]
  }
}
